{
  "id": "chronos",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "boritzio/docker-chronos",
      "network": "HOST",
      "parameters": [
        { "key": "env", "value": "MESOS_ZK=zk://192.168.43.10:2181/mesos" },
        { "key": "env", "value": "CHRONOS_ZK=192.168.43.10:2181" }
      ]
    }
  },
  "instances": 1,
  "cpus": 0.5,
  "mem": 516,
  "ports": [4400]
}
